{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "import/no-extraneous-dependencies": 0,
        "no-shadow": "off",
        "no-unused-vars":"off",
        "no-restricted-globals":"off",
        "no-undef":"off",
        "import/prefer-default-export": "off",
        "prefer-destructuring": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off"
    }
}
